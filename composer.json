{
  "name": "one-strive/nova-image-cropper",
  "description": "An advanced image field for Nova with crop and resize. Security-focused fork addressing critical JS dependency vulnerabilities.",
  "keywords": [
    "laravel",
    "nova",
    "nova5",
    "image",
    "crop",
    "resize",
    "image crop",
    "image resize",
    "advanced",
    "advanced image",
    "advanced image field",
    "security",
    "security-update"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "intervention/image-laravel": "^1.3",
    "laravel/nova": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "OneStrive\\NovaImageCropper\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "OneStrive\\NovaImageCropper\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
